{
  "name": "super-vote-secret",
  "version": "2.0.0",
  "description": "SUPER moteur de vote secret - Application moderne de vote anonyme pour assemblÃ©es",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && python -m uvicorn server:app --host 0.0.0.0 --port 8001 --reload",
    "dev:frontend": "cd frontend && yarn start",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && yarn build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:backend": "cd backend && python -m uvicorn server:app --host 0.0.0.0 --port 8001",
    "start:frontend": "cd frontend && serve -s build -l 3000",
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "install:frontend": "cd frontend && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/vote-secret.git"
  },
  "keywords": [
    "voting",
    "assembly",
    "secret-vote",
    "fastapi",
    "react",
    "mongodb",
    "anonymous",
    "pdf-report"
  ],
  "author": "Your Name",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/your-username/vote-secret/issues"
  },
  "homepage": "https://github.com/your-username/vote-secret#readme",
  "devDependencies": {
    "concurrently": "^9.1.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0",
    "python": ">=3.11.0"
  }
}